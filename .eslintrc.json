{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/function-component-definition": "off",
        "semi":"off",
        "react/prop-types": "off",
        "no-param-reassign":"off",
        "linebreak-style":"off",
        "no-console":"off",
        "consistent-return":"off"
    }
}
